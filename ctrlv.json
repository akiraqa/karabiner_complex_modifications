{
  "title": "Obsidian paste by Ctrl-v",
  "rules": [
    {
      "description": "Obsidian paste by Ctrl-v",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": [
                  "left_command"
                ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^md\\.obsidian$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
