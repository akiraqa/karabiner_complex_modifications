                                "left_shift",
                                "left_control"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "Ctrl+5 to switch tab",
                    "type": "basic",
                    "from": {
                        "key_code": "5",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}