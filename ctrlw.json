{
    "title": "BrowserControl by Ctrl(w/f/l/r/t/←/→/1/9/4/5)",
    "docs":"https://karabiner-elements.pqrs.org/docs/json/",
    "rules": [
        {
            "description": "BrowserControl by Ctrl(w/f/l/r/t/←/→/1/9/4/5)",
            "manipulators": [
                {
                    "description": "ctrl+w  to close tab",
                    "type": "basic",
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "w",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.google\\.Chrome$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.brave\\.Browser$",
                                "^com\\.microsoft\\.VSCode$",
                                "^md\\.obsidian$"
                            ]
                        }
                    ]
                },
                {
                    "description": "ctrl+f  to find",
                    "type": "basic",
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.google\\.Chrome$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.brave\\.Browser$",
                                "^md\\.obsidian$"
                            ]
                        }
                    ]
                },
                {
                    "description": "ctrl+l  to go to address bar",
                    "type": "basic",
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "l",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "ctrl+r  to reload",
                    "type": "basic",
                    "from": {
                        "key_code": "r",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "r",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "t",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "t",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "option + ← to back",
                    "type": "basic",
                    "from": {
                        "key_code": "left_arrow",
                        "modifiers": {
                            "mandatory": [
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "option + → to go ahead",
                    "type": "basic",
                    "from": {
                        "key_code": "right_arrow",
                        "modifiers": {
                            "mandatory": [
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "Ctrl+1 to switch most left tab",
                    "type": "basic",
                    "from": {
                        "key_code": "1",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "1",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "Ctrl+9 to switch lastest tab",
                    "type": "basic",
                    "from": {
                        "key_code": "9",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "9",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.brave\\.Browser$",
                                "^com\\.apple\\.Safari$",
                                "^com\\.google\\.Chrome$"                            
                            ]
                        }
                    ]
                },
                {
                    "description": "Ctrl+4 to switch prev tab",
                    "type": "basic",
                    "from": {
                        "key_code": "4",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
